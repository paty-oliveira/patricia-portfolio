{
  "name": "patricia-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && npm run generate-sitemap ",
    "preview": "vite preview",
    "prepare": "husky",
    "lint": "eslint . --fix --max-warnings=0",
    "format": "prettier --check 'src/**/*.{js,ts,tsx,json,css,md}' --write",
    "type-check": "tsc --noEmit",
    "test": "jest .",
    "test-changed-files": "jest . --findRelatedTests --passWithNoTests --bail",
    "pre-commit": "npm run lint && npm run format && npm run type-check && npm run test",
    "generate-sitemap": "node generate-sitemap.cjs"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.26.0",
    "@emotion/react": "^11.14.0",
    "axios": "^1.11.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.8.2",
    "rehype-raw": "^7.0.0",
    "vike": "^0.4.239",
    "vike-react": "^0.6.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "fs": "^0.0.1-security",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "jest-transformer-svg": "^2.1.0",
    "path": "^0.12.7",
    "prettier": "3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.5",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
